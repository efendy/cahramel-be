{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "User Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "last_name": {
      "pluginOptions": {},
      "type": "string"
    },
    "email_address": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "phone_number": {
      "type": "biginteger"
    },
    "user_contracts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-contract.user-contract",
      "mappedBy": "user_profile"
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "notset",
        "male",
        "female"
      ],
      "default": "notset"
    },
    "marital_status": {
      "type": "enumeration",
      "enum": [
        "single",
        "married",
        "divorced",
        "widowed",
        "separated"
      ],
      "default": "single"
    },
    "nationality": {
      "type": "string"
    },
    "race": {
      "type": "enumeration",
      "enum": [
        "notset",
        "others",
        "chinese",
        "malay",
        "indian",
        "eurasian"
      ],
      "default": "notset"
    },
    "image_profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "religion": {
      "type": "enumeration",
      "enum": [
        "notset",
        "buddhism",
        "hinduism",
        "islam",
        "christianity",
        "others"
      ],
      "default": "notset"
    },
    "registered_companies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::company-profile.company-profile",
      "mappedBy": "registered_by"
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    }
  }
}
