{
  "kind": "collectionType",
  "collectionName": "company_profiles",
  "info": {
    "singularName": "company-profile",
    "pluralName": "company-profiles",
    "displayName": "Company Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "uen": {
      "type": "string",
      "required": true
    },
    "company_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company-type.company-type",
      "inversedBy": "company_profile"
    },
    "departments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::department.department",
      "mappedBy": "company_profile"
    },
    "is_verified": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "parent_company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company-profile.company-profile",
      "inversedBy": "child_companies"
    },
    "child_companies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::company-profile.company-profile",
