    "company_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company-profile.company-profile"
    },
    "job_title": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::job-title.job-title"
    },
    "reporting_to_main": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-contract.user-contract"
    },
    "reporting_to_secondary": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-contract.user-contract"
    },
    "date_start": {
      "type": "date"
    },
    "date_end": {
      "type": "date"
    },
    "email_address": {
      "type": "email"
    },
    "employee_id": {
      "type": "string"
    },
    "department": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department"
    },
    "access_role": {
      "type": "enumeration",
      "enum": [
        "user",
        "admin",
        "owner"
      ],
