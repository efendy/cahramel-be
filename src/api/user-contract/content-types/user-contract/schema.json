      "type": "date"
    },
    "complete_offboarding_by": {
      "type": "date"
    },
    "user_profile": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-profile.user-profile",
      "inversedBy": "user_contracts"
